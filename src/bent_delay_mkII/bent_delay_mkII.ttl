@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix lv2:     <http://lv2plug.in/ns/lv2core#> .
@prefix lv2ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix lv2atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2time: <http://lv2plug.in/ns/ext/time#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .


<http://ssj71.github.io/infamousPlugins/plugs.html#bent_delay_mkII>
        a lv2:Plugin, lv2:DelayPlugin ;
        doap:name "the infamous bent delay MK II" ;
        doap:license <http://opensource.org/licenses/GPL-3.0> ;
        doap:description "delay with randomly varying feedback amount";
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://ssj71.github.io/infamousPlugins> ;
                foaf:mbox <mailto:ssjackson71@gmail.com> ;
        ] ;
        lv2:minorVersion 0 ;
        lv2:microVersion 1 ;
        rdfs:comment "This otherwise basic delay just has a randomly varying feedback" ;
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index  2 ;
                lv2:symbol "TIME" ;
                lv2:name "Delay" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1400.0 ;
                units:unit units:ms ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "DELAY_RANGE" ;
                lv2:name "Delay Range" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 500.0 ;
                units:unit units:ms ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "DELAY_RATE" ;
                lv2:name "Delay Randomness" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 120.0 ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "RATE" ;
                lv2:name "FB Randomness" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 120.0 ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "FB_MIN" ;
                lv2:name "Feedback Minimum" ;
                lv2:default 0.0 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "FB_MAX" ;
                lv2:name "Feedback Maximum" ;
                lv2:default 0.8 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ].
